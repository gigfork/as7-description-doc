<h4><a href="#{{  breadcrumb }}" id="{{ breadcrumb }}">{{ name  }} - {{ breadcrumb }}</a></h4>

{% comment %}

<p>{{ operation.description }}</p>

<h5>Request properties</h5>
  <ul>
  {% for prop in operation.request-properties %}
    <li>{{ prop[0] }}: {{ prop[1].description }}</li>
    <ul><li>{{ prop[1].type.TYPE_MODEL_VALUE}}</li></ul>
  {% endfor %}
  </ul>
<p>Returns {{ operation.reply-properties.description }}</p>
<p>Returned type: {{ operation.reply-properties.type.TYPE_MODEL_VALUE }} </p>

{% endcomment %}

{%comment %}
<p>{{ operation }}</p>
{%endcomment %}
